mylist = [21, 5, 8, 52, 21, 87]
r_item = 21
for item in mylist:
    if(item==r_item):
        mylist.remove(r_item)

print(mylist)


n=int(input("Enter a number: "))
for i in range(0,n):
    for j in range(0,n):
        if(i==j):
            print("1",sep=" ",end=" ")
        else:
            print("0",sep=" ",end=" ")
    print()
    
    
    
    
    list=[]
for i in range(0,4):
    a=input()
    list.append(a)
list.sort()
print(list)
list.sort(reverse=True)
print(list)
    
    
    
    
    res1=[]
res2=[]
list2=['abc','xyz',1,2,'pqr',3,4,'a']
for j in list2:
    if(type(j)==int):
        res1.append(j)
    elif(type(j)==str):
        res2.append(j)
print("Integer list:  " +str(res1))
print("String List: " + str(res2))




import collections
L = [1, 2, 8, 5, 8, 7, 8]
largest = max(L)
res = collections.Counter(L)
print(res[largest])


tuples = 2, 4, 5, 6, 2, 3, 4, 4, 7 
print(tuples)
count = tuples.count(4)
print(count)



size = int(input("Enter the size of the matrix : "))
for i in range(0,size):
    for j in range(0,size):
        if(i==j):
            print("1",end = " ")
        else:
            print("0",end = " ")
        print()
        
        
        
        order = int(input("Enter order of matrix: "))
for i in range(0, order):  
    for j in range(0, order):
        if i == j :
            print("1", end = " ")
        else:
            print("0", end = " ")
    print()
    
    
