num = [10, 30, 40, 20, 50]
num.sort(reverse=True)
print (num)
fnum = [10.23, 10.12, 20.45, 11.00, 0.1]
fnum.sort(reverse=True)
print (fnum)
str = ["Banana", "Cat", "Apple", "Dog", "Fish"]
str.sort(reverse=True)
print (str)



